{
  "version": "0.2.0",
  "configurations": [
    {
      "cwd": "${workspaceRoot}",
      "type": "cortex-debug",
      "request": "launch",
      "name": "cmsis-dap-debug",
      "servertype": "openocd",
      // 这个是根据主文件名 作为名字 ${workspaceFolderBasename}
      "executable": "build/${workspaceFolderBasename}.elf",
      "runToEntryPoint": "main",
      "configFiles": [
        "interface/cmsis-dap.cfg",
        "target/mspm0.cfg"
      ],
      "liveWatch": {
        "enabled": true,
        "samplesPerSecond": 20
      }
    },
    {
      "cwd": "${workspaceRoot}",
      "type": "cortex-debug",
      "request": "launch",
      "name": "jlink-debug",
      "servertype": "jlink",
      "interface": "swd", // 改成jtag也可以
      // 这个是根据主文件名 作为名字 ${workspaceFolderBasename}
      "executable": "build/${workspaceFolderBasename}.elf",
      "runToEntryPoint": "main",
      "device": "MSPM0G3507",
      "svdFile": "Flash/MSPM0G350X.svd",
      "liveWatch": {
          "enabled": true,
          "samplesPerSecond": 20,
      },
    }
  ]
}